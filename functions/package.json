{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run build-next && npm run serve",
    "deploy-webAPI": "npm run build && firebase deploy --only functions:webAPI",
    "deploy-webAPIV4": "npm run build && firebase deploy --only functions:webAPIV4",
    "deploy-next": "npm run build-next-web && npm run build && firebase deploy --only functions:nextjsFunc",
    "deploy-hosting": "firebase deploy --only hosting",
    "build-next-mobile": "next build ./src/client/mobile",
    "build-next-web": "next build ./src/client/web",
    "dev-next-mobile": "next dev ./src/client/mobile",
    "dev-next-web": "next dev ./src/client/web",
    "deploy-v4": "npm run deploy-next && npm run deploy-hosting && npm run deploy-webAPIV4"
  },
